{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "single"],
    "arrow-body-style": 0,
    //Line 19 might not work
    "function-component-definition": "off",
    "react/prop-types": 0
  }
}

// 0 - disable error, 1 - warning, 2 - error
